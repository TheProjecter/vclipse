import org.vclipse.vcml.vcml.*

diagram MaterialDiagram type Material {
	
	node MaterialNode for this {
		label name for this
		edge Depedency for each this.billofmaterials.iterator.next.items {
			=> call BomItemNode for this
		}
	}
	
	node BomItemNode for this.billofmaterials.iterator.next.items {
